friends = ["Alice", "Bob", "Charlie","David","Eva","Frank", "Grace"]
#slice = friends[0:2]
print (friends[0])
print (friends[1])
print (friends[2])

#addition
friends.append("Dave")
print (friends)

#revesre
friends.reverse()

#insert
friends.insert(2,"Zoe") 
print (friends)

#remove
friends.remove("Bob")
print (friends)

#pop
friends.pop()
print (friends)

#sort
friends.sort()
print (friends)

#length
print (len(friends))    

#clear  
friends.clear()
print (friends)
